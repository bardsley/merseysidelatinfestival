CLERK_SECRET_KEY=<From Cleark.io>
NEXT_PUBLIC_CLERK_SIGN_UP_URL=/auth/sign-up
NEXT_PUBLIC_CLERK_SIGN_IN_URL=/auth/sign-in

# URLS for server actions
LAMBDA_GETATTENDEES=<From AWS>
LAMBDA_PREFERENCES=<From AWS>
LAMBDA_SEND_TICKET_EMAIL=<From AWS>
