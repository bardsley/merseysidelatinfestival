# These are retrieved from your project at app.tina.io
NEXT_PUBLIC_TINA_CLIENT_ID=<From Tina.io>
TINA_TOKEN=<From Tina.io>

# Debug and information
NEXT_PUBLIC_INTERNAL_DEBUG=<true|false> # Show debug on pages
NEXT_PUBLIC_TINA_BRANCH=<develop|main|branch> # This is set by default CI with Netlify/Vercel/Github, but can be overriden
VERCEL_ENV=preview # To see the preview functionality

# Stripe
STRIPE_SECRET_KEY=<From Stripe>
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=<From Stripe>

# Instgram
IG_TOKEN=<From Instagram>
INSTAGRAM_APP_ID=<From Instagram>
